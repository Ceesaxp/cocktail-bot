# PostgreSQL settings
POSTGRES_PASSWORD=secure_postgresql_password

# MongoDB settings
MONGODB_PASSWORD=secure_mongodb_password

# API settings
# Comma-separated list of API tokens
API_TOKENS=token1,token2,token3