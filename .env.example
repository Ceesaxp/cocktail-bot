# Docker image settings
REGISTRY=ceesaxp
TAG=latest

# Telegram bot settings
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_from_botfather
TELEGRAM_BOT_USERNAME=your_bot_username

# Database credentials
# MySQL settings
MYSQL_ROOT_PASSWORD=secure_root_password
